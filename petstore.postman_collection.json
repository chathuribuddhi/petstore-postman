						{
							"name": "Find pet by ID",
							"event": [
								{
									"listen": "test",
									"script": {
										"exec": [
											"pm.test(\"Verify request is successful with a status code of 200 if the petID is valid\", function () {",
											"  pm.response.to.have.status(200);",
											"});",
											"",
											"pm.test(\"Verify Test data type of the response\", () => {",
											"    const jsonData = pm.response.json();",
											"  pm.expect(jsonData).to.be.an(\"object\");",
											"  pm.expect(jsonData.id).to.be.a(\"number\");",
											"  pm.expect(jsonData.category).to.be.an(\"object\");",
											"  pm.expect(jsonData.category.id).to.be.an(\"number\");",
											"  pm.expect(jsonData.category.name).to.be.an(\"string\");",
											"  pm.expect(jsonData.name).to.be.a(\"string\");",
											"  pm.expect(jsonData.photoUrls).to.be.an(\"array\");",
											"  pm.expect(jsonData.tags).to.be.an(\"array\");",
											"  pm.expect(jsonData.status).to.be.a(\"string\");",
											"});",
											"",
											"pm.test(\"Verify pet ID is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.id).to.eql(8979789078978911);",
											"});",
											"",
											"pm.test(\"Verify Category ID is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.category.id).to.eql(111);",
											"});",
											"",
											"pm.test(\"Verify Pet Category is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.category.name).to.eql(\"akita dog\");",
											"});",
											"",
											"pm.test(\"Verify Pet Name is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.name).to.eql(\"doggie\");",
											"});",
											"",
											"pm.test(\"Verify Photo URL is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.photoUrls).to.eql([\"photo URL\"]);",
											"});",
											"",
											"pm.test(\"Verify Tag is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.tags).to.eql([]);",
											"});",
											"",
											"pm.test(\"Verify Status is correctly appearing\", () => {",
											"  const responseJson = pm.response.json();",
											"  pm.expect(responseJson.status).to.eql(\"available\");",
											"});",
											""
										],
										"type": "text/javascript"
									}
								}
